local player = game.Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()
local humanoid = character:FindFirstChildOfClass("Humanoid")

if humanoid then
    humanoid.WalkSpeed = 50
    humanoid.JumpPower = 130

    
    humanoid:GetPropertyChangedSignal("FloorMaterial"):Connect(function()
        if humanoid.FloorMaterial == Enum.Material.Air then
wait(1)
            game.Workspace.Gravity = 1000
wait(0.3)
game.Workspace.Gravity = 150
        end
    end)
end

button.MouseButton1Click:Connect(function()
local player = game.Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()
local humanoid = character:FindFirstChildOfClass("Humanoid")

if humanoid then
    humanoid:ChangeState(Enum.HumanoidStateType.Jumping)
end
wait(1)
humanoid.Sit = true
end)

button1.MouseButton1Click:Connect(function()
local Humanoidroot = Game.Players.LocalPlayer.Character.HumanoidRootPart
Humanoidroot.Anchored = true
button1.Visible = false
button2.Visible = true
end)

button2.MouseButton1Click:Connect(function()
Game.Players.LocalPlayer.Character.HumanoidRootPart.Anchored = false
button2.Visible = false
button1.Visible = true
end)

button3.MouseButton1Click:Connect(function()
Game.Players.LocalPlayer.Character.Humanoid.HipHeight = 10
button3.Visible = false
button4.Visible = true
end)

button4.MouseButton1Click:Connect(function()
Game.Players.LocalPlayer.Character.Humanoid.HipHeight = 1.998
button4.Visible = false
button3.Visible = true
end)

button5.MouseButton1Click:Connect(function()
local lol = 0
button5.Visible = false
button6.Visible = true
while wait() do
local character = Game.Players.LocalPlayer.Character
character.HumanoidRootPart.CanCollide = false
character.UpperTorso.CanCollide = false
character.LowerTorso.CanCollide = false
if lol == 1 then
break
end
if button6.Visible == true then
lol = lol + 1
end
end
end)

button6.MouseButton1Click:Connect(function()
button5.Visible = true
button6.Visible = false
wait(0.1)
local character = Game.Players.LocalPlayer.Character
character.HumanoidRootPart.CanCollide = true
character.UpperTorso.CanCollide = true
character.LowerTorso.CanCollide = true
end)
button7.MouseButton1Click:Connect(function()
if frame.Visible == true then
frame.Visible = false
elseif frame.Visible == false then
frame.Visible = true
end
end)

button8.MouseButton1Click:Connect(function()
Game.Workspace.Camera.FieldOfView = 100
button8.Visible = false
button9.Visible = true
end)
button9.MouseButton1Click:Connect(function()
Game.Workspace.Camera.FieldOfView = 70
button8.Visible = true
button9.Visible = false
end)

button10.MouseButton1Click:Connect(function()
button.Visible = false
button1.Visible = false
button2.Visible = false
button3.Visible = false
button4.Visible = false
button5.Visible = false
button6.Visible = false
button8.Visible = false
button9.Visible = false
button10.Visible = false
button11.Visible = true
button13.Visible = true
button15.Visible = true
button17.Visible = true
end)

button11.MouseButton1Click:Connect(function()
humanoid.WalkSpeed = 16
button11.Visible = false
button12.Visible = true
end)

button12.MouseButton1Click:Connect(function()
humanoid.WalkSpeed = 50
button12.Visible = false
button11.Visible = true
end)

button13.MouseButton1Click:Connect(function()
humanoid.JumpPower = 50
button13.Visible = false
button14.Visible = true
end)

button14.MouseButton1Click:Connect(function()
humanoid.JumpPower = 130
button14.Visible = false
button13.Visible = true
end)

local stomp = 1

button15.MouseButton1Click:Connect(function()
local humanoid = Game.Players.LocalPlayer.Character.Humanoid
button15.Visible = false
button16.Visible = true
stomp = 1
while wait() do
humanoid.HipHeight = 3.333
wait(0.1)
humanoid.HipHeight = 1.998
wait()
humanoid.Sit = true
wait(0.3)
humanoid.Sit = false
if stomp == 2 then
break
end
end
end)

button16.MouseButton1Click:Connect(function()
stomp = 2
button16.Visible = false
button15.Visible = true
end)

button17.MouseButton1Click:Connect(function()
button.Visible = true
button1.Visible = true
button3.Visible = true
button5.Visible = true
button7.Visible = true
button9.Visible = true
button10.Visible = true
button11.Visible = false
button12.Visible = false
button13.Visible = false
button14.Visible = false
button15.Visible = false
button16.Visible = false
button17.Visible = false
end)
